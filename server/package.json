{
	"name": "server",
	"version": "0.0.1",
	"description": "do shit with ai",
	"main": "./dist/index.js",
	"author": "Priya X. Pramesi",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"format": "prettier --write \"src\"",
		"build": "yarn clean & tsc",
		"start": "node ./dist/index.js",
		"clean": "rimraf dist/",
		"auth:create:user": "node ./helpers/register_user_knex.cjs",
		"auth:set:imap": "node ./helpers/set_user_imap_settings.cjs",
		"knex:migrate:make": "node --loader ts-node/esm ./node_modules/.bin/knex migrate:make",
		"knex:migrate:latest": "node --loader ts-node/esm ./node_modules/.bin/knex migrate:latest",
		"knex:migrate:up": "node --loader ts-node/esm ./node_modules/.bin/knex migrate:up",
		"knex:migrate:rollback": "node --loader ts-node/esm ./node_modules/.bin/knex migrate:rollback --all",
		"test:single": "NODE_OPTIONS=--experimental-vm-modules yarn run jest --config jest.config.cjs --testTimeout 100000"
	},
	"dependencies": {
		"bcrypt": "^5.1.0",
		"bunyan": "^1.8.15",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"imap": "^0.8.19",
		"jsonwebtoken": "^9.0.1",
		"knex": "^2.5.1",
		"langchain": "^0.0.145",
		"langsmith": "^0.0.26",
		"lodash": "^4.17.21",
		"mailparser": "^3.6.5",
		"node-schedule": "^2.1.1",
		"p-retry": "^5.1.2",
		"pg": "^8.11.1",
		"rimraf": "^5.0.1",
		"uuid": "^9.0.0",
		"zod": "^3.21.4",
		"zod-to-json-schema": "^3.21.4"
	},
	"devDependencies": {
		"@jest/globals": "^29.6.4",
		"@tsconfig/recommended": "^1.0.2",
		"@types/bcrypt": "^5.0.0",
		"@types/bunyan": "^1.8.8",
		"@types/express": "^4.17.17",
		"@types/imap": "^0.8.37",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/lodash": "^4.14.195",
		"@types/mailparser": "^3.4.0",
		"@types/node": "^20.4.3",
		"@types/node-schedule": "^2.1.0",
		"@types/p-retry": "^3.0.1",
		"@types/uuid": "^9.0.2",
		"jest": "^29.6.4",
		"minimist": "^1.2.8",
		"prettier": "^3.0.0",
		"supabase": "^1.88.0",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.6"
	}
}
